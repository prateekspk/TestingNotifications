<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prateek.testingnotifications">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icl_spk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icl_spk"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="stateVisible|adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InnerActivity"
            android:windowSoftInputMode="stateVisible|adjustResize" />
        <activity android:name=".ShowHistoryActivity" />
        <activity android:name=".RecyclerVIew"></activity>
    </application>

</manifest>